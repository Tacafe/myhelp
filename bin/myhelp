#!/usr/bin/env ruby

puts "Hello main"
#
# add lib folder
$:.unshift(File.dirname(__FILE__) + '/../lib')
require 'arg_parser'
require 'file_loader'

module MyHelp
  class << self
    def get_helpfile
      puts "hello get_helpfile"
    end

    def main 
      ArgParser.parse()
      puts "test"
      p FileLoader.pick("h", false)
    end
  end

end

MyHelp.main()
