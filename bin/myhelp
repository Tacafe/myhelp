puts $:
require 'parser_arg'

module MyHelp
  class << self
    def get_helpfile
      puts "hello get_helpfile"
    end

    def choose
      puts "hello choose"
    end

    def main 
      self.get_helpfile
      self.choose
    end
  end

end

MyHelp.main()

ParserArg.Hello()



