#!/usr/bin/env ruby

puts "Hello main"
#
# add lib folder
$:.unshift(File.dirname(__FILE__) + '/../lib')
require 'input_parameter'
require 'file_loader'

module MyHelp
  class << self
    def get_helpfile
      puts "hello get_helpfile"
    end

    def main 
      puts "start"
      ip = InputParameter.new
      p ip.command
      p ip.option
      p ip.target
      p ip.grep
      p ip.prev_filename
      p ip.new_filename
      p ip.is_valid
    end
  end

end

MyHelp.main()
